{
  "name": "nwb",
  "description": "A toolkit for React, Preact & Inferno apps, React libraries and other npm modules for the web, with no configuration (until you need it)",
  "version": "0.25.2",
  "license": "MIT",
  "author": "Jonny Buchanan <jonathan.buchanan@gmail.com>",
  "bin": {
    "nwb": "./lib/bin/nwb.js"
  },
  "scripts": {
    "build": "npm run flow && npm run lint && rimraf lib && babel src --out-dir lib --quiet --copy-files",
    "build:watch": "rimraf lib && babel src --watch --out-dir lib --copy-files",
    "flow": "flow",
    "lint": "eslint *.js src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build && npm run test:all",
    "test:coverage": "npm run build && cross-env NODE_ENV=test NWB_TEST=true nyc mocha tests/*-test.js tests/commands/*-test.js",
    "test:all": "cross-env NODE_ENV=test NWB_TEST=true mocha --require @babel/register tests/*-test.js tests/commands/*-test.js",
    "test:watch": "cross-env NODE_ENV=test NWB_TEST=true mocha --require @babel/register --watch tests/*-test.js"
  },
  "files": [
    "lib",
    "templates",
    "express.js"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "copy-template-dir": "1.4.0",
    "cross-spawn": "7.0.3",
    "debug": "4.3.1",
    "detect-port": "1.3.0",
    "figures": "3.2.0",
    "filesize": "6.3.0",
    "fs-extra": "9.1.0",
    "gzip-size": "6.0.0",
    "inquirer": "8.0.0",
    "minimist": "1.2.5",
    "ora": "5.4.0",
    "resolve": "1.20.0",
    "run-series": "1.1.9",
    "semver": "7.3.5",
    "webpack-merge": "4.2.2",

    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-jsx": "7.12.13",
    "@babel/plugin-transform-react-constant-elements": "7.13.13",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.14.0",
    "@babel/preset-react": "7.13.13",
    "@babel/runtime": "7.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-inferno": "6.2.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-proposals": "0.4.0",
    "react-refresh": "0.10.0",

    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",

    "mocha": "8.3.2",
    "expect": "1.20.2",
    "process" : "0.11.10",

    "webpack": "5.36.1",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "copy-webpack-plugin": "8.1.1",
    "css-loader": "5.2.4",
    "css-minimizer-webpack-plugin": "2.0.0",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "mini-css-extract-plugin": "1.5.1",
    "@insin/npm-install-webpack-plugin": "5.0.0",
    "open": "8.0.7",
    "postcss": "8.4.31",
    "postcss-loader": "5.2.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.1",
    "url-loader": "4.1.1",
    "webpack-dev-middleware": "4.1.0",
    "webpack-dev-server": "3.11.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "7.13.13",
    "@babel/register": "7.13.16",
    "cross-env": "7.0.3",
    "eslint-config-jonnybuchanan": "6.0.0",
    "eventsource": "1.1.0",
    "flow-bin": "0.130.0",
    "glob": "7.1.6",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "temp": "0.9.4",
    "tree-kill": "1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insin/nwb.git"
  },
  "keywords": [
    "development",
    "inferno",
    "preact",
    "react",
    "tooling"
  ],
  "babel": {
    "targets": {
      "node": "10.13.0"
    },
    "presets": [
      "@babel/flow",
      "@babel/env",
      ["babel-preset-proposals", {
        "classProperties": {
          "loose": true
        }
      }]
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "plugins": [
          ["istanbul", {
            "include": "src"
          }]
        ]
      }
    }
  },
  "nyc": {
    "instrument": false,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  }
}
